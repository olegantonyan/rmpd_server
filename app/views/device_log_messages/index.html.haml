= page_title(@device_log_messages)

= render_filter_form do |f|
  = filter_form_datetime(f, :with_since_date, format: 'DD.MM.YYYY', label: 'Since date')
  = filter_form_datetime(f, :with_to_date, format: 'DD.MM.YYYY', label: 'To date')

= render 'collection'

= link_to icon_link_to('export', 'CSV'), device_device_log_messages_path(device_id: params[:device_id], format: 'csv'), class: 'btn btn-default'
